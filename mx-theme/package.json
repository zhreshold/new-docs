  },
  "homepage": "https://github.com/myyasuda/sphinx_materialdesign_theme#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.6.2",
    "rimraf": "^2.6.2",
    "sass": "^1.14.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "material-design-lite": "^1.3.0",
    "parcel": "^1.10.3"
